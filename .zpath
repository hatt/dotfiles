# Homebrew path
export PATH="/usr/local/bin:$PATH"
export PATH="/usr/local/sbin:$PATH"

# crap for Go
export GOPATH="$HOME/code/go"
export PATH="$GOPATH/bin:$(brew --prefix go)/libexec/bin:$PATH"

source $GOPATH/src/github.com/vmware/govmomi/scripts/govc_bash_completion

# script envs
export RBENV_ROOT=~/.rbenv
if type plenv &> /dev/null; then eval "$(plenv init -)"; fi
if type rbenv &> /dev/null; then eval "$(rbenv init -)"; fi
if type pyenv &> /dev/null; then eval "$(pyenv init -)"; fi
if type nodenv &> /dev/null; then eval "$(nodenv init -)"; fi
